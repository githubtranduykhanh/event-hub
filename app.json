{
  "expo": {
    "name": "eventhub",
    "slug": "eventhub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/expo-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/expo-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.eventhub",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.eventhub"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "db1c5fd5-eec5-4143-838d-e58c27de67b3"
      }
    },
    "scheme": "eventhub"
  }
}
